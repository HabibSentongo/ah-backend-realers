{
  "addons": [
    "heroku-postgresql"
  ],
  "buildpacks": [
    {
      "url": "heroku/python"
    }
  ],
  "env": {
    "DB_HOST": {
      "required": true
    },
    "DB_NAME": {
      "required": true
    },
    "DB_PASS": {
      "required": true
    },
    "DB_PORT": {
      "required": true
    },
    "DB_USER": {
      "required": true
    },
    "DISABLE_COLLECTSTATIC": {
      "required": true
    },
    "IS_DEBUG_ON": {
      "required": true
    },
    "SECRET_KEY": {
      "required": true
    },
    "SECURITY_PASSWORD_SALT": {
      "required": true
    },
    "EMAIL_HOST":{
      "required": true
    },
    "HOST_PASSWORD": {
      "required": true
    },
    "EMAIL_USE_TLS":{
      "required": true
    },
    "EMAIL_PORT":{
      "required": true
    },
    "EMAIL_HOST_USER":{
      "required": true
    },
    "EMAIL_HOST_PASSWORD":{
      "required":true
    }
  },
  "formation": {
    "web": {
      "quantity": 1
    }
  },
  "name": "ah-backend-realers",
  "scripts": {
    "postdeploy": "python manage.py makemigrations && python manage.py migrate"
  },
  "stack": "heroku-18"
}
